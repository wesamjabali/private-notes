\`;

const tokens = md.parse(text, {});
console.log('Tokens:');
tokens.forEach((t, i) => {
  console.log(i, t.type, t.tag, JSON.stringify(t.map), JSON.stringify(t.content?.substring(0, 50)));
  if (t.children) {
    t.children.forEach((c, j) => {
      console.log('  child', j, c.type, JSON.stringify(c.content?.substring(0, 50)));
    });
  }
});
